datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model TestProfile {
  id   String @id @default(uuid())
  name String
}

model Product {
  id          String   @id @default(uuid())
  name        String
  company     String
  description String
  featured    Boolean
  image       String
  price       Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  clerkId     String
}
